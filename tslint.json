{
  "rules": {
    "no-empty": false,
    "indent": [true, "spaces"],
    "max-classes-per-file": false,
    "no-duplicate-variable": true,
    "one-line": [
      true,
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": [true, "double"],
    "semicolon": [true, "never"],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ]
  }
}
