{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": false,
    "rootDir": "src/main/assets",
    "outFile": "target/sbt-typescript/typescriptc.js",
    "baseUrl": ".",
    "paths": {
      "*": [
        "*",
        "target/web/node-modules/main/webjars/fs-extra/*",
        "node_modules/@types/node/*"
      ]
    },
    "lib": ["es6"],
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "preserveConstEnums": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "typeRoots": [
      "target/web/node-modules/main/webjars/@types",
      "node_modules/@types"
    ]
  },
  "exclude": [
    "example",
    "node_modules",
    "test",
    "src/sbt-test"
  ]
}
